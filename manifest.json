{
  "name": "QuoteTabs",
  "version": "1.0",
  "description": "Displays a quote about math or computer science on every new tab",
  "permissions": ["tabs"],
  "chrome_url_overrides": {"newtab": "newtab.html"},

  "icons": {
  "16": "images/img16.png",
  "32": "images/img32.png",
  "48": "images/img48.png",
  "128": "images/img128.png"
  },
  "manifest_version": 2
}
